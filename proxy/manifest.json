{
    "manifest_version": 2,
    "name": "Modify Headers",
    "version": "1.0",
    "description": "Intercept and modify HTTP headers",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    }
  }
  